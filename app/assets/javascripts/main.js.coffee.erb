angular.module('HashBangURLs', []).config(['$locationProvider', ($location) ->
  $location.hashPrefix('!');
])

@logicalicy = angular.module('logicalicy', ['ngRoute', 'ngResource', 'HashBangURLs'])

@logicalicy.config(['$routeProvider', ($routeProvider) ->
  $routeProvider.
  when('/labs', {
    templateUrl: "<%= asset_path('templates/apps/index.html') %>",
    controller: 'AppIndexCtrl'
  }).
  otherwise({
    templateUrl: "<%= asset_path('templates/home.html') %>",
    controller: 'HomeCtrl'
  })
])